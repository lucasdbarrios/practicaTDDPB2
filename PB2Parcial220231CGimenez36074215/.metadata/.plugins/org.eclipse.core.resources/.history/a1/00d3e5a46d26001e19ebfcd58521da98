package ar.edu.unlam.pb2.eva03;

public class Submarino implements Acuatico {

	
	private Integer codigoSubmarino;
	private String nombre;
	private Double profundidad;
	public Submarino(Integer codigoSubmarino, String nombre) {
		this.codigoSubmarino=codigoSubmarino;
		this.nombre=nombre;
		this.profundidad=0.0;
	}

	@Override
	public Double getProfundidad() {
		// TODO Auto-generated method stub
		return this.profundidad;
	}

}
